<p style="color: green"><%= notice %></p>

<h1>Items</h1>

<%= render 'import' %>

<div id="items">
  <table class="table">

  <tbody>
  <% @items.each do |item| %>
    <%= render item %>
    <p>
      <%= link_to "", item %>
    </p>
  <% end %>
  </tbody>
  </div>
</div>

<%= link_to "New item", new_item_path %>
